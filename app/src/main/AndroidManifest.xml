<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jun.luxhomedialer" >
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.SEND_SMS"/>
    <uses-permission android:name="android.permission.RECEIVE_SMS"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_splash"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".views.SystemPickerActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>                                                                                      n
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".views.SystemEditorActivity"
            android:label="@string/title_sys_edit"
            android:screenOrientation="portrait">
        </activity>
        <activity android:name=".views.HomeActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"/>
        <activity android:name=".views.SystemSettingsActivity"
            android:label="@string/cat_three"
            android:screenOrientation="portrait"/>
        <!--
        <activity android:name=".views.AlarmActivity"
            android:label="@string/title_alarm"
            android:screenOrientation="portrait"/>
        <activity android:name=".views.LangSettingActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_lang"/>
            -->
        <activity android:name=".views.RfidActivity"
            android:label="@string/title_rfid_tag"
            android:screenOrientation="portrait"/>
        <activity android:name=".views.RfidSubActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".views.NotificationSettingsActivity"
            android:label="@string/cat_two"
            android:screenOrientation="portrait"/>
        <activity android:name=".views.GenericNumbersListAcitivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".views.NumberEditorActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".views.RingSettingsActivity"
            android:screenOrientation="portrait"
            android:label="@string/icon_siren"/>
        <activity android:name=".views.ZoneGridActivity"
            android:screenOrientation="portrait"
            android:label="@string/zone_names"/>
        <activity android:name=".views.ZoneSubActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".views.DelayActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_delays"/>
    </application>
</manifest>
